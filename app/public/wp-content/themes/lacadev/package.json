  "private": true,
  "engines": {
    "node": ">=20.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --color --config resources/build/webpack.development.js",
    "build": "cross-env NODE_ENV=production webpack --color --config resources/build/webpack.production.js",
    "lint": "run-s -cl lint:wpcs lint:scripts lint:styles",
    "lint-fix": "run-s -cl lint-fix:wpcs lint-fix:scripts lint-fix:styles",
    "i18n": "run-s -cl i18n:textdomain i18n:pot",
    "release": "run-s -cl build release:zip",
    "release:zip": "node resources/build/release/release.js",
    "critical": "critical http://localhost:8000 --base dist --width 1300 --height 900 --target styles/critical.css --extract"
  },
  "browserslist": [
    "last 3 versions"
  ],
